{
    "name": "baywa-re-lusy/email",
    "description": "BayWa r.e. LUSY Email Tools",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/baywa-re-lusy/email",
    "require": {
        "php": ">= 8.1",
        "mailgun/mailgun-php": "^3.5",
        "nyholm/psr7": "^1.4",
        "aws/aws-sdk-php": "^3.253"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^1.9",
        "behat/behat": "^3.19"
    },
    "autoload": {
        "psr-4": {
            "BayWaReLusy\\Email\\": "src/Email"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BayWaReLusy\\Email\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
